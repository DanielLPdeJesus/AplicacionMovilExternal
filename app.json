{
  "expo": {
    "name": "ExternalGlowApp",
    "slug": "ExternalGlowApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconresulocion.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashredimencioando.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icono-noti.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "@expo/vector-icons/Ionicons"
          ]
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconresulocion.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.danielglow.ExternalGlowApp",
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS",
        "VIBRATE"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "b93697d8-e068-440a-9d90-a873c0aa07bb"
      }
    }
  }
}